{
  "expo": {
    "name": "Praticamente",
    "slug": "PraticamenteApp",
    "version": "2.1.0",
    "orientation": "portrait",
    
    "icon": "./assets/adaptive-icon-no-name.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f4f6fc"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.lsm00.praticamente"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-no-name.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "blockedPermissions": [
        "android.permission.ACTIVITY_RECOGNITION"
      ],
      "package": "com.lsm_00.PraticamenteApp"
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "O Praticamente precisa de acesso a suas fotos para compartilhar."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/adaptive-icon-no-name.png",
          "color": "#f4f6fc",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-audio",
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "df49ac5a-738c-4ddc-b55f-f31355bd03be"
      }
    }
  }
}
