{
  "expo": {
    "name": "TEA App",
    "slug": "TEAApp",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon":  "./assets/adaptive-icon-no-name.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f4f6fc"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-no-name.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.lsm_00.TEAApp"
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "O TEAApp precisa de acesso a suas fotos para compartilhar."
        }
        
      ],
      [
        "expo-notifications",
        {
          "icon":  "./assets/adaptive-icon-no-name.png",
          "color": "#f4f6fc",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ]
      
      
      
    ],
    "extra": {
      "eas": {
        "projectId": "dae164a0-a2fb-4df0-a5ad-7cb92ee4147c"
      }
    }
  }
}
